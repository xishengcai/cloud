                "ip": {
                    "type": "string"
                },
                "password": {
                    "type": "string"
                },
                "port": {
                    "type": "integer",
                    "default": 22
                },
                "proxyPort": {
                    "type": "integer",
                    "default": 20000
                },
                "user": {
                    "type": "string",
                    "default": "root"
                }
